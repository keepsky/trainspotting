modelentry ri from b1 {
  exit sa
  length 500.0
}

modelexit reright to b2 {
  entry s1
  entrysection a2
  length 800.0
}

modelexit releft to b2 {
  entry s2
  entrysection a2
  length 800.0
}

route rright {
  entry sa exit s1
  entrysection a1
  length 800.0
  sections [a1]
  switches [sw1 right]
  contains []
}

route rleft {
  entry sa exit s2
  entrysection a1
  length 800.0
  sections [a1]
  switches [sw1 left]
  contains []
}

